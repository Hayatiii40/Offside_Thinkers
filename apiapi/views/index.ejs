<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Clubs</title>
  <style>
    body{
      display: flex;
      flex-direction: column;
      align-items: center;
      
    }
    ul {
      list-style: none;
      padding: 0;
    }
    div{
      border: 2px solid black;
      display: flex;
      padding: 10px;
      align-items: center;
    }
    img {
      height: 40px;
      width: 40px;
      
      margin-right: 12px;
    }
  </style>
</head>
<body>
  <h1>Football Clubs</h1>
  <ul>
    <% clubs.forEach(club => { %>
      <% if (club.name!== "Common") { %>
       <div>
        <img src="<%= club.logoUrl %>" alt="<%= club.name %> logo" width="500" />
        <p><%= club.name %></p>
      </div>
      <% } %>
      
    <% }); %>
    
  </ul>
  
</body>
</html>
